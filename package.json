    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.4.1",
    "glob": "^7.2.3",
    "gulp": "^4.0.2",
    "gulp-imagemin": "^7.1.0",
    "gulp-noop": "^1.0.1",
    "gulp-postcss": "^9.0.0",
    "gulp-rev": "^9.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-svg-sprite": "^1.5.0",
    "gulp-tap": "^2.0.0",
    "gulp-uglify-es": "^3.0.0",
    "lighthouse": "^5.6.0",
    "loose-envify": "^1.4.0",
